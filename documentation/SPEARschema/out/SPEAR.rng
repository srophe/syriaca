               <sch:let name="qrURI"
                        value="$ti//listURI[@type = 'qualifier-relationship']//uri"/>
               <sch:let name="error" value="."/>
               <sch:assert test="                       every $i in (tokenize(., ' '))                       satisfies $i = $qrURI">
