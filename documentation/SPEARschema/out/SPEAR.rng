   <define name="person">
      <element name="person">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(person) provides information about an identifiable individual, for example a participant in a language interaction, or a person referred to in a historical source. [13.3.2. The Person Element 15.2.2. The Participant Description]</a:documentation>
         <choice>
            <ref name="persName"/>
            <group>
               <ref name="persName"/>
               <ref name="birth"/>
            </group>
            <group>
               <ref name="persName"/>
               <ref name="death"/>
            </group>
            <group>
               <ref name="persName"/>
               <ref name="education"/>
            </group>
            <group>
               <ref name="persName"/>
               <optional>
                  <ref name="nationality"/>
               </optional>
            </group>
            <group>
               <ref name="persName"/>
               <ref name="residence"/>
            </group>
            <group>
               <ref name="persName"/>
               <ref name="occupation"/>
            </group>
            <group>
               <ref name="persName"/>
               <ref name="langKnowledge"/>
            </group>
            <group>
               <ref name="persName"/>
               <ref name="state"/>
            </group>
            <group>
               <ref name="persName"/>
               <optional>
                  <ref name="trait"/>
               </optional>
            </group>
            <group>
               <ref name="persName"/>
