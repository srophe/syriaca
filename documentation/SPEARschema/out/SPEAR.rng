               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(education) Used to designate person factoids that encode education</a:documentation>
               <value>ethnicLabel</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(ethnic label) Used to designate person factoids that encode ethnic labels</a:documentation>
               <value>event</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(event) Used to designate event factoids</a:documentation>
               <value>gender</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(gender) Used to designate person factoids that encode gender</a:documentation>
               <value>langKnown</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(language known) Used to designate person factoids that encode language knowledge</a:documentation>
               <value>mentalState</value>
