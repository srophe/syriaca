  <remarks xmlns:rng="http://relaxng.org/ns/structure/1.0" xmlns:sch="http://purl.oclc.org/dsdl/schematron" xmlns:srophe="https://srophe.app" xmlns:tei="http://www.tei-c.org/ns/1.0" versionDate="2016-12-01" xml:lang="en">
      <p>Any positive integer value or zero is permitted</p>
  </remarks>
  <remarks xmlns:rng="http://relaxng.org/ns/structure/1.0" xmlns:sch="http://purl.oclc.org/dsdl/schematron" xmlns:srophe="https://srophe.app" xmlns:tei="http://www.tei-c.org/ns/1.0" xml:lang="ja" versionDate="2008-04-05">
