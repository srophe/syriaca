                <ptr target="http://syriaca.org/bibl/668"/>
                <citedRange unit="p">151</citedRange>
              </bibl>
            </egXML>
          </exemplum><exemplum xmlns:rng="http://relaxng.org/ns/structure/1.0">
            <egXML xmlns="http://www.tei-c.org/ns/Examples">
              <titleStmt>
                <title level="a" xml:lang="en">Telneshe — <foreign xml:lang="syr">ܬܠܢܫܐ</foreign>
                </title> ... <editor role="code-author" ref="http://syriaca.org/documentation/editors.xml#tcarlson">Thomas A.
                  Carlson</editor>
                <editor role="content-author" ref="http://syriaca.org/documentation/editors.xml#dboero">Dina Boero</editor>
                <respStmt>
                  <resp>Initial XML by</resp>
                  <name ref="http://syriaca.org/documentation/editors.xml#tcarlson">Thomas A.
