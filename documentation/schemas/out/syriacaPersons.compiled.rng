                  following values: <sch:value-of select="string-join($targetVals, ' | ')"/>.
                </sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="syriacaPersons-link-linkTargets3-constraint-assert-96">
