   </define>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="syriacaAll-att.global.change-change-att-values-constraint-rule-123">
      <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:srophe="https://srophe.app"
                context="//@change">
         <sch:let name="changeIDs" value="//tei:teiHeader//tei:change/@xml:id"/>
         <sch:let name="changeIDpointers"
                  value="for $i in $changeIDs return concat('#', $i)"/>
         <sch:assert test="                  every $i in tokenize(., ' ')                  satisfies $i = $changeIDpointers">
                  This @change attribute can contain one or more of the following <sch:value-of select="$changeIDpointers"/>.
                </sch:assert>
      </sch:rule>
   </pattern>
   <define name="tei_att.global.linking.attributes">
      <ref name="tei_att.global.linking.attribute.corresp"/>
   </define>
   <define name="tei_att.global.linking.attribute.corresp">
      <optional>
         <attribute name="corresp">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(corresponds) points to elements that correspond to the current element in some way.</a:documentation>
            <list>
               <oneOrMore>
                  <data type="anyURI">
                     <param name="pattern">\S+</param>
                  </data>
               </oneOrMore>
            </list>
         </attribute>
      </optional>
   </define>
   <start>
      <ref name="tei_TEI"/>
   </start>
</grammar>
