            <zeroOrMore>
               <ref name="tei_title"/>
            </zeroOrMore>
            <text/>
         </interleave>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="syriacaAll-bibl-mssCitation-constraint-rule-54">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:srophe="https://srophe.app"
                      context="//tei:text//tei:desc//tei:bibl | //tei:text//tei:note//tei:bibl">
               <sch:assert test="@type='syriaca-ms'">
                  Must be @type='syriaca-ms'. If not a mss, use &lt;title&gt;.
                </sch:assert>
               <sch:assert test="tei:ptr/@target">
                  This &lt;bibl&gt; must have a &lt;ptr&gt; element with @target attribute.
                </sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:srophe="https://srophe.app"
                      context="//tei:text//tei:desc//tei:bibl/tei:ptr/@target | //tei:text//tei:note//tei:bibl/tei:ptr/@target">
               <sch:assert test="matches(., concat('http://syriaca.org/manuscript/', '\d+$'))">
                  This @target attribute must point to a Syriaca.org manuscript URI taking the form "http://syriaca.org/manuscript/{\d+$}} where {\d+$} is a number.
                </sch:assert>
            </sch:rule>
         </pattern>
         <ref name="tei_att.global.attribute.xmlid"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">characterizes the element in some sense, using any convenient classification scheme or typology.</a:documentation>
               <choice>
                  <value>syriaca-ms</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">used for bibliographic citation of a manuscript</a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
