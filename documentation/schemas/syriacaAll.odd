              <sch:rule context="//tei:body/tei:listPlace/tei:place/tei:bibl">
                <sch:let name="docURIno" value="//tei:publicationStmt/tei:idno/substring-before(substring-after(substring-after(., 'http://syriaca.org/'), '/'), '/tei')"/>
