    <memberOf key="att.global"/>
    <memberOf key="model.phrase.xml"/>
  </classes><content xmlns:rng="http://relaxng.org/ns/structure/1.0" xmlns:xi="http://www.w3.org/2001/XInclude">
    <dataRef key="teidata.name"/>
  </content><attList><attDef xmlns:rng="http://relaxng.org/ns/structure/1.0" xmlns:xi="http://www.w3.org/2001/XInclude" ident="scheme" usage="opt">
